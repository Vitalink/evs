<authenticationConfiguration>
    <evs>
        <type>fallback</type>
        <certificates>
            <certificate>
                <type>identification</type>
                <path>..\exe\certificates\[NAME_OF_ACC_P12_CERTIFICATE_FILE]</path>
                <password>[CERTIFICATE_PASSWORD]</password>
            </certificate>
        </certificates>
    </evs>
    <ehealth>
        <entry>kmehr.hubservicev3.software.id.local.value.1=[YOUR_VITALINK_APPLICATION_IDENTIFICATION_CODE]</entry>
        <entry>user.name=[RETIREMENTHOME-NAME]</entry>
        <entry>user.nihii=[RETIREMENTHOME-NIHII]</entry>
        <entry>encryption.actor.id=${user.nihii}</entry>
        <entry>encryption.actor.cd=NIHII</entry>
        <entry>main.kmehr.quality=orgpublichealth</entry>
        <entry>kmehr.default.hcpartylist=identifier</entry>
        <entry>kmehr.hubservicev3.hcpartylist=identifier,software</entry>
        <entry>kmehr.hubservicev3.identifier.id.idhcparty.value=${user.nihii}</entry>
        <entry>kmehr.hubservicev3.identifier.id.idhcparty.sv=1.0</entry>
        <entry>kmehr.hubservicev3.identifier.cd.cdhcparty.value=orgpublichealth</entry>
        <entry>kmehr.hubservicev3.identifier.cd.cdhcparty.sv=1.1</entry>
        <entry>kmehr.hubservicev3.identifier.id.idencryptionkey=false</entry>
        <entry>kmehr.hubservicev3.identifier.id.idencryptionkey.sv=1.0</entry>
        <entry>kmehr.hubservicev3.identifier.name=${user.name}</entry>
        <entry>kmehr.hubservicev3.identifier.address.cd.sv=1.0</entry>
        <entry>kmehr.hubservicev3.identifier.address.cd.value=</entry>
        <entry>kmehr.hubservicev3.identifier.address.cd.schemes=cdaddress</entry>
        <entry>kmehr.hubservicev3.identifier.address.nis=24062</entry>
        <entry>sessionmanager.samlattribute.1=urn:be:fgov:identification-namespace,urn:be:fgov:ehealth:1.0:retirement:nihii-number,${user.nihii}</entry>
        <entry>sessionmanager.samlattribute.2=urn:be:fgov:identification-namespace,urn:be:fgov:ehealth:1.0:certificateholder:retirement:nihii-number,${user.nihii}</entry>
        <entry>sessionmanager.samlattributedesignator.1=urn:be:fgov:identification-namespace,urn:be:fgov:ehealth:1.0:certificateholder:retirement:nihii-number</entry>
        <entry>sessionmanager.samlattributedesignator.2=urn:be:fgov:identification-namespace,urn:be:fgov:ehealth:1.0:retirement:nihii-number</entry>
        <entry>sessionmanager.samlattributedesignator.3=urn:be:fgov:certified-namespace:ehealth,urn:be:fgov:ehealth:1.0:certificateholder:retirement:nihii-number:recognisedretirement:boolean</entry>
        <entry>sessionmanager.samlattributedesignator.4=urn:be:fgov:certified-namespace:ehealth,urn:be:fgov:ehealth:1.0:retirement:nihii-number:recognisedretirement:nihii11</entry>
    </ehealth>
</authenticationConfiguration>