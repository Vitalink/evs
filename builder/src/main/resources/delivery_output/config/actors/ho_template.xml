<authenticationConfiguration>
    <evs>
        <type>fallback</type>
        <certificates>
            <certificate>
                <type>identification</type>
                <path>..\exe\certificates\[NAME_OF_ACC_P12_CERTIFICATE_FILE]</path>
                <password>[CERTIFICATE_PASSWORD]</password>
            </certificate>
        </certificates>
    </evs>
    <ehealth>
        <entry>kmehr.hubservicev3.software.id.local.value.1=[YOUR_VITALINK_APPLICATION_IDENTIFICATION_CODE]</entry>
        <entry>user.nihii=[HOSPITAL-NIHII]</entry>
        <entry>user.name=[HOSPITAL-NAME]</entry>
        <entry>careprovider.inss=[PHYSICIAN-SSIN]</entry>
        <entry>careprovider.nihii=[PHYSICIAN-NIHII11]</entry>
        <entry>careprovider.firstname=[PHYSICIAN-FIRSTNAME]</entry>
        <entry>careprovider.lastname=[PHYSICIAN-LASTNAME]</entry>
        <entry>encryption.actor.id=${user.nihii}</entry>
        <entry>encryption.actor.cd=NIHII_HOSPITAL</entry>
        <entry>main.kmehr.quality=orghospital</entry>
        <entry>kmehr.default.hcpartylist=identifier</entry>
